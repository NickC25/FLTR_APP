<%= form_tag({:controller => "filter", :action => "index"}, :method => "get") do %>
  <%= text_field_tag :search %>
  <%= submit_tag "search" %>
<% end %>


<% if @has_result %>
  <% @videos.each do |video|  %>
  https://www.youtube.com/watch?v=<%= video.id %>
  <embed width="420" height="315" src="https://www.youtube.com/embed/<%= video.id %>">
  <% end %>
<% end %>
